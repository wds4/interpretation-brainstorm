{
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Credentials", "value": "true" },
                { "key": "Access-Control-Allow-Origin", "value": "*" },
                { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
                { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
            ]
        }
    ],
    "functions": {
        "pages/api/requestInterpretation/*.ts": {
            "maxDuration": 180
        },
        "src/pages/api/requestInterpretation/*.ts": {
            "maxDuration": 180
        },
        "pages/api/nostr/updateFollowsAndMutesNextUserBlock/index.ts": {
            "maxDuration": 180
        },
        "pages/api/nostr/inputFollowsIntoDbNextUserBlock/index.ts": {
            "maxDuration": 180
        },
        "pages/api/nostr/query/singleUser/dos/index.ts": {
            "maxDuration": 180
        },
        "src/pages/api/nostr/updateFollowsAndMutesNextUserBlock/index.ts": {
            "maxDuration": 180
        },
        "src/pages/api/nostr/inputFollowsIntoDbNextUserBlock/index.ts": {
            "maxDuration": 180
        },
        "src/pages/api/nostr/query/singleUser/dos/index.ts": {
            "maxDuration": 180
        },
        "pages/api/*.ts": {
            "maxDuration": 60
        },
        "pages/api/nostr/query/singleUser/ratingsTable/*.ts": {
            "maxDuration": 180
        },
        "src/pages/api/nostr/query/singleUser/ratingsTable/*.ts": {
            "maxDuration": 180
        }
    }
}
